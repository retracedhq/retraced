    "pkg": "5.6.0",
    "tslint": "5.20.1",
    "typemoq": "2.1.0",
    "typescript": "4.6"
  },
  "scripts": {
    "pretest": "tslint --project ./tsconfig.json",
    "test": "mocha --recursive -r ts-node/register src/test/**/*.ts",
    "cover": "nyc mocha",
    "mintest": "mocha --recursive -r ts-node/register --reporter 'min' src/test/**/*.ts",
    "report-coverage": "coveralls < ./coverage/lcov.info && codeclimate-test-reporter < ./coverage/lcov.info",
    "snyk-protect": "snyk protect",
    "start:api": "node --inspect=0.0.0.0 -r ts-node/register src/index.ts",
    "start:processor": "node --inspect=0.0.0.0 -r ts-node/register src/_processor/index.ts"
  },
  "author": "",
  "snyk": true,
  "nyc": {
    "extension": [
      ".ts",
      ".js"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/services/templates/**/*",
      "node_modules/**/*"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true
  },
  "pkg": {
    "scripts": [
      "migrations/es/**/*",
      "migrations/pg/**/*"
    ],
    "assets": [
      "node_modules/@elastic/elasticsearch/api/**/*"
    ]
  },
  "bin": {
    "run": "index.js"
  }
}
