{
  "name": "retraced",
  "version": "1.5.0",
  "description": "A fully open source audit log service and embeddable UI easily deployed to your own Kubernetes cluster",
  "main": "./build/index.js",
  "license": "Apache 2.0",
  "dependencies": {
    "@elastic/elasticsearch": "7.17.0",
    "@retracedhq/retraced": "0.5.1",
    "analytics-node": "6.2.0",
    "axios": "1.2.2",
    "bcrypt": "5.0.1",
    "@bugsnag/js": "7.18.0",
    "@bugsnag/plugin-express": "7.18.0",
    "chance": "1.1.9",
    "cli-spinner": "0.2.10",
    "cors": "2.8.5",
    "csv-string": "4.1.0",
    "csv-stringify": "6.2.3",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "graphql": "16.6.0",
    "handlebars": "4.7.7",
    "jsonwebtoken": "9.0.0",
    "left-pad": "1.1.3",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.40",
    "monkit": "0.4.0",
    "nodemailer": "6.9.0",
    "nodemailer-mandrill-transport": "1.2.0",
    "nsqjs": "0.13.0",
    "pg": "8.7.3",
    "pg-cursor": "2.7.4",
    "pg-format": "1.0.4",
    "picocolors": "1.0.0",
    "pino": "8.7.0",
    "postgrator": "3.0.0",
    "progress": "2.0.3",
    "prom-client": "14.1.0",
    "redis": "4.2.0",
    "ruler": "1.0.0",
    "sanitize-filename": "1.6.3",
    "search-query-parser": "1.6.0",
    "sigsci-module-nodejs": "2.1.3",
    "ssl-config": "1.0.0",
    "swagger-ui-express": "4.6.0",
    "tsoa": "5.0.0",
    "unzipper": "0.10.11",
    "uuid": "8.3.2",
    "walk": "2.3.15",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@testdeck/mocha": "0.3.3",
    "@types/bcrypt": "5.0.0",
    "@types/elasticsearch": "5.0.40",
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.191",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.13",
    "@types/pg": "8.6.5",
    "@types/pg-types": "1.11.5",
    "@types/statsd-client": "0.4.3",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/uuid": "8.3.4",
    "@types/yargs": "17.0.17",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.6.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "rimraf": "4.1.1",
    "snyk": "1.1069.0",
    "swagger-jsdoc": "6.2.8",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "tv4": "1.3.0",
    "typemoq": "2.1.0",
    "typescript": "4.9.4"
  },
  "scripts": {
    "swagger": "tsoa spec",
    "routes": "tsoa routes",
    "prebuild": "rimraf build",
    "build": "npm run swagger && npm run routes && tsc && cp build/src/retracedctl.js build/src/retracedctl && chmod +x build/src/retracedctl",
    "lint": "eslint -c .eslintrc.js ./src",
    "test": "mocha --recursive -r ts-node/register src/test/**/*.ts",
    "integration:test": "mocha --recursive -r ts-node/register integration/test/**/*.ts",
    "e2e:test": "mocha --recursive -r ts-node/register src/e2e-tests/**/*.ts",
    "cover": "nyc mocha",
    "mintest": "mocha --recursive -r ts-node/register --reporter 'min' src/test/**/*.ts",
    "snyk-protect": "snyk protect",
    "dev:api": "node --inspect=0.0.0.0 -r ts-node/register src/index.ts",
    "dev:processor": "PROCESSOR_DEV_PORT=3001 node --inspect=0.0.0.0 -r ts-node/register src/_processor/index.ts",
    "dev:geo": "node --inspect=0.0.0.0 -r ts-node/register src/_processor/workers/updateGeoData.ts",
    "dev": "docker compose up -d",
    "dev:skaffold": "skaffold dev --status-check=false --force=true"
  },
  "author": "",
  "snyk": true,
  "nyc": {
    "extension": [
      ".ts",
      ".js"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/services/templates/**/*",
      "node_modules/**/*"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true
  },
  "bin": {
    "run": "index.js"
  }
}