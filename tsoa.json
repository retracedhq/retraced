{
  "entryFile": "./src/index.ts",
  "controllerPathGlobs": ["./src/**/*Controller.ts"],
  "spec": {
    "outputDirectory": "build",
    "specVersion": 3,
    "spec": {
      "servers": [
        {
          "url": "http://localhost:3000/auditlog",
          "description": "Local"
        }
      ]
    }
  },
  "swagger": {
    "outputDirectory": "./build",
    "host": "localhost:3000"
  },
  "routes": {
    "middleware": "express",
    "routesDir": "./src/gen",
    "middlewareTemplate": "./src/gen/express.tstmpl"
  }
}
